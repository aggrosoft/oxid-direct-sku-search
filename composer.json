{
  "name": "aggrosoft/oxid-direct-sku-search",
  "description": "Redirect user to product if sku matches in search and no other results found",
  "type": "oxideshop-module",
  "keywords": ["oxid", "modules", "eShop"],
  "homepage": "https://github.com/aggrosoft/oxid-direct-sku-search",
  "license": "MIT",
  "extra": {
    "oxideshop": {
      "blacklist-filter": [
        "documentation/**/*.*"
      ],
      "target-directory": "agdirectskusearch"
    }
  },
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Aggrosoft\\DirectSkuSearch\\": "../../../source/modules/agdirectskusearch"
    }
  }
}